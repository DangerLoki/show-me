{
	"schemaVersion": 1,
	"id": "show-me",
	"version": "${version}",
	"name": "Show Me",
	"description": "Shows only the information you want on the HUD (coords, ping, FPS, etc.) without needing F3.",
	"authors": [
		"Danger_Loki, RaijinBr"
	],
	"contact": {
		"homepage": "https://github.com/DangerLoki/mod-mine-show-me",
		"sources": "https://github.com/DangerLoki/mod-mine-show-me"
	},
	"license": "MIT",
	"icon": "assets/show-me/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			"com.meioQuilo.showme.ShowMeClient"
		]
	},
	"mixins": [
		"showme.mixins.json",
		{
			"config": "showme.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.17.0",
		"minecraft": ">=1.21.9 <=1.21.10",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}
